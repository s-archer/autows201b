{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,  
    "label": "my BIG-IP declaration for declarative onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "f5demo.f5.com",
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
            "8.8.8.8"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "time.google.com"
            ],
            "timezone": "UTC"
        },
        "public": {
            "class": "VLAN",
            "tag": 1001,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "public-self": {
            "class": "SelfIp",
            "address": "10.0.2.101/24",
            "vlan": "public",
            "allowService": "none",
            "trafficGroup": "traffic-group-local-only"
        },
        "private": {
            "class": "VLAN",
            "tag": 1002,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "private-self": {
            "class": "SelfIp",
            "address": "10.0.3.10/24",
            "vlan": "private",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "default": {
          "class": "Route",
          "gw": "10.0.2.1",
          "network": "default"
        },
        "internalVpc": {
          "class": "Route",
          "gw": "10.0.3.1",
          "network": "10.0.0.0/16"
        }
    }
}